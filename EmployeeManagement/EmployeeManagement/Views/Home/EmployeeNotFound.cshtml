@model int

@{
    ViewData["Title"] = "EmployeeNotFound";
}

<div class="alert alert-danger mt-1 mb-1">
    <h4>404 Not Found Error</h4>
    <hr/>
    <h4>Employee of id @Model not found</h4>
</div>
<a asp-controller="Home" asp-action="Index" class="btn btn-outline-success" style="width: auto">Click here to see the list of all employees</a>